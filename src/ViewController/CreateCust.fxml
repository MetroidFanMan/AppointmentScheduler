<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.CreateCustController">
   <children>
      <Label layoutX="85.0" layoutY="40.0" text="Create Customer">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="100.0" text="Name:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="150.0" text="Address:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="19.0" layoutY="200.0" text="Address 2:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="65.0" layoutY="250.0" text="City:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="300.0" text="Country:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="350.0" text="ZipCode:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="47.0" layoutY="400.0" text="Phone:">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameTxt" layoutX="130.0" layoutY="100.0" promptText="First Name" />
      <TextField fx:id="addrTxt" layoutX="130.0" layoutY="150.0" promptText="Address" />
      <TextField fx:id="addr2Txt" layoutX="130.0" layoutY="200.0" promptText="Optional" />
      <TextField fx:id="cityTxt" layoutX="130.0" layoutY="250.0" promptText="City" />
      <TextField fx:id="countryTxt" layoutX="130.0" layoutY="300.0" promptText="Country" />
      <TextField fx:id="zipTxt" layoutX="130.0" layoutY="350.0" promptText="#####" />
      <TextField fx:id="phoneTxt" layoutX="130.0" layoutY="400.0" promptText="###-###-####" />
      <Button fx:id="saveBtn" layoutX="125.0" layoutY="450.0" mnemonicParsing="false" onAction="#saveBtn" prefWidth="50.0" text="Save" />
   </children>
</AnchorPane>
